{
  "name": "lamc-components",
  "version": "1.0.20",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.js",
  "es2017": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/lamc-components/lamc-components.esm.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Luis-Alberto-Cisneros/demo-stencil.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec",
    "e2e": "stencil test --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate"
  },
  "exports": {
    ".": {
      "import": "./dist/components/index.js",
      "types": "./dist/components/index.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "types": "./loader/index.d.ts"
    }
  },
  "dependencies": {
    "@stencil/core": "^4.7.0"
  },
  "devDependencies": {
    "@stencil/sass": "^3.0.10",
    "@types/jest": "^29.5.6",
    "@types/node": "^16.18.11",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "puppeteer": "^21.9.0"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Luis-Alberto-Cisneros/demo-stencil/issues"
  },
  "homepage": "https://github.com/Luis-Alberto-Cisneros/demo-stencil#readme",
  "author": "Albert Cisneros"
}
